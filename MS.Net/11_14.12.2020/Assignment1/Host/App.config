<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

<system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>

      </serviceBehaviors>
    </behaviors>

    <services>
      <service behaviorConfiguration ="mexBehavior" name="WcfService.Service1">
        <endpoint address ="Service1" binding="netTcpBinding" contract="WcfService.IService1">
         
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress ="http://localhost:8080"/>
            <add baseAddress ="net.tcp://localhost:8090"/>
          </baseAddresses>
        </host>
      </service>
      
    </services>
  </system.serviceModel>
</configuration>